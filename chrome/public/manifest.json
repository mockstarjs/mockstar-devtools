{
  "name": "MockStar",
  "version": "1.0.0",
  "description": "MockStar 调试工具用于 mock server",
  "devtools_page": "devtools.html",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["file:///*", "http://*/*", "https://*/*"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
